server{
    server_name gao.mendela.xyz;
    location /media {
        alias /home/jurgeon/projects/gao/src/media;
    }
    location /static {
        alias /home/jurgeon/projects/gao/src/static_root;
    }
    location / {
        uwsgi_pass unix:/home/jurgeon/projects/gao/src/config/gao.sock;
        include /etc/nginx/uwsgi_params;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/gao.mendela.xyz/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/gao.mendela.xyz/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server{
    if ($host = gao.mendela.xyz) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name gao.mendela.xyz;
    return 404; # managed by Certbot


}